# Environment variables for production

# Server configuration
PORT=3000

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=admin
DB_PASSWORD=admin
DB_DATABASE=jpg_to_pdf

# JWT configuration
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=3600

# Google OAuth configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Microsoft OAuth configuration
MICROSOFT_CLIENT_ID=your_microsoft_client_id
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret
MICROSOFT_CALLBACK_URL=http://localhost:3000/auth/microsoft/callback

# Cloud storage configuration
CLOUD_STORAGE_BUCKET=your_cloud_storage_bucket
CLOUD_STORAGE_REGION=your_cloud_storage_region
CLOUD_STORAGE_ACCESS_KEY=your_cloud_storage_access_key
CLOUD_STORAGE_SECRET_KEY=your_cloud_storage_secret_key
